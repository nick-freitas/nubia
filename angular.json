{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      }
    },
    "api-model-chapter-api-model": {
      "root": "libs/api-model/chapter-api-model",
      "sourceRoot": "libs/api-model/chapter-api-model/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-model/chapter-api-model/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-model/chapter-api-model"],
          "options": {
            "jestConfig": "libs/api-model/chapter-api-model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-model-gamebook-api-model": {
      "root": "libs/api-model/gamebook-api-model",
      "sourceRoot": "libs/api-model/gamebook-api-model/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-model/gamebook-api-model/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-model/gamebook-api-model"],
          "options": {
            "jestConfig": "libs/api-model/gamebook-api-model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-model-progression-api-model": {
      "root": "libs/api-model/progression-api-model",
      "sourceRoot": "libs/api-model/progression-api-model/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-model/progression-api-model/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-model/progression-api-model"],
          "options": {
            "jestConfig": "libs/api-model/progression-api-model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-model-user-api-model": {
      "root": "libs/api-model/user-api-model",
      "sourceRoot": "libs/api-model/user-api-model/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-model/user-api-model/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-model/user-api-model"],
          "options": {
            "jestConfig": "libs/api-model/user-api-model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "data-access-gamebook-data-access": {
      "projectType": "library",
      "root": "libs/data-access/gamebook-data-access",
      "sourceRoot": "libs/data-access/gamebook-data-access/src",
      "prefix": "nubia",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data-access/gamebook-data-access"],
          "options": {
            "jestConfig": "libs/data-access/gamebook-data-access/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/data-access/gamebook-data-access/src/**/*.ts",
              "libs/data-access/gamebook-data-access/src/**/*.html"
            ]
          }
        }
      }
    },
    "nubia": {
      "projectType": "application",
      "root": "apps/nubia",
      "sourceRoot": "apps/nubia/src",
      "prefix": "nubia",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nubia",
            "index": "apps/nubia/src/index.html",
            "main": "apps/nubia/src/main.ts",
            "polyfills": "apps/nubia/src/polyfills.ts",
            "tsConfig": "apps/nubia/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/nubia/src/favicon.ico", "apps/nubia/src/assets"],
            "styles": ["apps/nubia/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/nubia/src/environments/environment.ts",
                  "with": "apps/nubia/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nubia:build:production"
            },
            "development": {
              "browserTarget": "nubia:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/nubia/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nubia:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nubia/src/**/*.ts",
              "apps/nubia/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nubia"],
          "options": {
            "jestConfig": "apps/nubia/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nubia-e2e": {
      "root": "apps/nubia-e2e",
      "sourceRoot": "apps/nubia-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nubia-e2e/cypress.json",
            "tsConfig": "apps/nubia-e2e/tsconfig.e2e.json",
            "devServerTarget": "nubia:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nubia:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nubia-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "nubia-reader": {
      "projectType": "application",
      "root": "apps/nubia-reader",
      "sourceRoot": "apps/nubia-reader/src",
      "prefix": "reader",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nubia-reader",
            "index": "apps/nubia-reader/src/index.html",
            "main": "apps/nubia-reader/src/main.ts",
            "polyfills": "apps/nubia-reader/src/polyfills.ts",
            "tsConfig": "apps/nubia-reader/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/nubia-reader/src/favicon.ico",
              "apps/nubia-reader/src/assets"
            ],
            "styles": ["apps/nubia-reader/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/nubia-reader/src/environments/environment.ts",
                  "with": "apps/nubia-reader/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nubia-reader:build:production"
            },
            "development": {
              "browserTarget": "nubia-reader:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/nubia-reader/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nubia-reader:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nubia-reader/src/**/*.ts",
              "apps/nubia-reader/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nubia-reader"],
          "options": {
            "jestConfig": "apps/nubia-reader/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nubia-reader-e2e": {
      "root": "apps/nubia-reader-e2e",
      "sourceRoot": "apps/nubia-reader-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nubia-reader-e2e/cypress.json",
            "tsConfig": "apps/nubia-reader-e2e/tsconfig.e2e.json",
            "devServerTarget": "nubia-reader:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nubia-reader:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nubia-reader-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "nubia"
}
